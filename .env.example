# Venice.ai API Key (https://venice.ai/chat?ref=VB8W1j)
# Required for accessing language models
VENICE_API_KEY=your_venice_api_key_here

# Brave Search API Key (https://api.search.brave.com/app)
# Optional - only needed if using Brave search provider
# Venice search is also available (uses Venice AI's built-in web search)
BRAVE_API_KEY=your_brave_search_api_key_here

# Search Provider (optional)
# Options: brave, venice
# Default: brave
# Venice search uses AI-grounded web search with citations
SEARCH_PROVIDER=brave

# Use Structured Outputs (optional)
# Options: true, false
# Default: true
# Enables JSON schema-based structured outputs for better data quality
# Falls back to text parsing if model doesn't support it
USE_STRUCTURED_OUTPUTS=true
