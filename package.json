    "test": "tsx --env-file=.env.test --test tests/ai/text-splitter.test.ts tests/search/search.test.ts",
    "test:delay": "tsx --env-file=.env tests/delay.ts",
    "test:search": "tsx --env-file=.env tests/search.ts"
  },
