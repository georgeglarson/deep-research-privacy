{
  "name": "deep-research-privacy-edition",
  "version": "1.1.1",
  "description": "Privacy-first AI research tool powered by Venice.ai's 10+ uncensored language models and Brave's private search. Features structured outputs, confidence scoring, parallel execution, and automatic citations. No data collection, no content restrictions.",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "start": "tsx --env-file=.env src/run.ts",
    "docker": "tsx src/run.ts",
    "models": "tsx src/list-models.ts",
    "test:venice-search": "tsx --env-file=.env src/test-venice-search.ts",
    "test:structured": "tsx --env-file=.env src/test-structured-outputs.ts",
    "docker:start": "tsx src/run.ts",
    "docker:models": "tsx src/list-models.ts",
    "docker:test:venice-search": "tsx src/test-venice-search.ts",
    "docker:test:structured": "tsx src/test-structured-outputs.ts"
  },
  "author": "george.g.larson@pm.me",
  "license": "ISC",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/node": "^22.13.0",
    "esbuild": "^0.25.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgeglarson/deep-research-privacy.git"
  },
  "homepage": "https://github.com/georgeglarson/deep-research-privacy#readme",
  "bugs": {
    "url": "https://github.com/georgeglarson/deep-research-privacy/issues"
  },
  "originalProject": {
    "name": "deep-research",
    "url": "https://github.com/dzhng/deep-research",
    "author": "https://github.com/dzhng"
  },
  "overrides": {
    "tsx": {
      "esbuild": "^0.25.0"
    }
  }
}
