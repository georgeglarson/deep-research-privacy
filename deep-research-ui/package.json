    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
