{
  "name": "deep-research-ui",
  "version": "1.0.0",
  "description": "Terminal UI for visualizing deep-research process",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "deep-research-ui": "./dist/server.js"
  },
  "exports": {
    ".": {
